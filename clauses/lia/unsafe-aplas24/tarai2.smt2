(set-logic HORN)
(declare-fun tarai2 (Int Int Int) Bool)

(assert (forall ((x Int) (y Int) (r Int))
  (=> (and (<= x y) (= r y))
    (tarai2 x y r))))
(assert (forall ((x Int) (y Int) (r Int) (r2 Int)(r3 Int))
  (=> (and (> x y) 
  	(tarai2 (- x 1) y r2)
  	(tarai2 (- y 1) x r3)
	(tarai2 r2 r3 r))
  (tarai2 x y r)
  )))
(assert (forall ((x Int))
  (=> (tarai2 30000 20000 x) (= x 200))))
(check-sat)
